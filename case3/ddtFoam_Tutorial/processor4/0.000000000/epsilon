FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.000000000";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 5.3;

boundaryField
{
    wand
    {
        type            compressible::epsilonWallFunction;
        value           uniform 86;
    }
    frontAndBack
    {
        type            empty;
    }
    procBoundary4to3
    {
        type            processor;
        value           uniform 5.3;
    }
    procBoundary4to5
    {
        type            processor;
        value           uniform 5.3;
    }
}


